[t:template,n:3W_dsx_shadow_minion_base]
{
	category_name = "3W_evil_b";
	doc = "3W_dsx_shadow_minion_base";
	specializes = actor_evil;
	[aspect]
	{
		experience_value = 1083250;
		life             = 1840.9;
		max_life         = 1840.9;
		mana             = 1840.9;
		max_mana         = 1840.9;
		model = m_c_ebb_sj_pos_1;
		material = flesh;
		[voice]
		{
			[die]			{ priority = high; * = s_e_die_shadowjumper_minion_SED; }
			[hit_critical]	{ priority = low; * = s_e_hit_skeleton_SED; }
			[hit_glance]	{ priority = low; * = s_e_hit_skeleton_SED; }
			[hit_solid]		{ priority = low; * = s_e_hit_skeleton_SED; }
		}		
	}
	[body]
	{
		avg_move_velocity = 2.5;
		[bone_translator]
		{
			body_anterior = bip01_head;
			body_mid = bip01_spine2;
			body_posterior = bip01_pelvis;
			kill_bone = bip01_spine2;
			shield_bone = shield_grip;
			weapon_bone = weapon_grip;
		}
		[weapon_scales]
		{
			as_single_melee = 1.5, 1.5, 1.5;
		}
		[chore_dictionary]
		{
			chore_prefix = a_c_ebb_sj_fs;
			[chore_attack]
			{
				chore_stances = 0;
				skrit = select_attack;
				[anim_files]
				{
					00 = at;
					01 = at-02;
					02 = at-03;
				}
			}
			[chore_default]
			{
				chore_stances = 0;
				skrit = infinite_loop;
				[anim_files] { 00=ds; }
			}
			[chore_die]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files] { 00=di; }
			}
			[chore_fidget]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files] 
				{ 
					00=dff; 
					01=dff-02;
				}
			}
			[chore_walk]
			{
				chore_stances = 0;
				skrit = select_walk;
				[anim_files] { 00=rl; }
			}
			[chore_misc]
			{
				chore_stances = 0;
				skrit = select_transition;

				[anim_files]
				{
					att1 = at;
					att2 = at-02;
					att3 = at-03;
					defa = ds;
					fid1 = dff;
					fid2 = dff-02;
				}
			}
		}
	}
	[conversation] { }
	[common]
	{
		//screen_name = "Shadowjumper Minion Base";
		membership = monster;
	}
	[defend] { defense = 1840.9; }

	[mind]
	{
		jat_brain = world\ai\jobs\common\brain_hero.skrit
			?actor_joins_existing_party	= true;

		jat_talk = world\ai\jobs\common\job_talk.skrit;
		jat_fidget = world\ai\jobs\common\job_fidget.skrit;
		melee_engage_range = 14;
		personal_space_range = 0.7;
		ranged_engage_range = 14.0;
		sight_range = 14.0;
	}
}

