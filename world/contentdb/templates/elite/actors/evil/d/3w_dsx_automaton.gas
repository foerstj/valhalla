[t:template,n:3W_dsx_automaton_base]
{
	//this currently has level 10 easy pcontent, so all will get that.  if
	//some automaton end up not being level 10 easy, they will need more
	//specific pcontent.

	category_name = "3W_evil_d";
	doc = "3W_dsx_automaton_base";
	specializes = actor_evil;
	[aspect]
		{
			model = m_c_edm_au;
			[voice]
				{
					[die]			{ priority = high; * = s_e_die_automaton;	 }
					[enemy_spotted]	{ priority = high; * = s_e_call_mechanoid_big_SED; }
				}
		}
	[body]
		{
			avg_move_velocity = 2;
			[bone_translator]
				{
					body_anterior = bip01_head;
					body_mid = bip01_spine1;
					body_posterior = bip01_pelvis;
					kill_bone = bip01_spine1;
					shield_bone = Bip01_L_Hand;
					weapon_bone = Bip01_R_Hand;
				}
			[chore_dictionary]
				{
					chore_prefix = a_c_edm_au_fs;
					[chore_attack]
						{
							chore_stances = 0;
							skrit = select_attack;
							[anim_files]
								{
									00 = at;
									01 = at-01;
								}
						}
					[chore_default]
						{
							chore_stances = 0;
							skrit = infinite_loop;
							[anim_files] { 00=ds; }
						}
					[chore_die]
						{
							chore_stances = 0;
							skrit = select_transition;
							[anim_files] { 00=di; }
						}
					[chore_fidget]
						{
							chore_stances = 0;
							skrit = select_fidget;
							[anim_files] { 00=dsf; }
						}
					[chore_walk]
						{
							chore_stances = 0;
							skrit = select_walk;
							[anim_files] { 00=wl; }
						}
					[chore_misc]
						{
							chore_stances = 0;
							skrit = select_transition;
							[anim_files] 
								{
									slep=ds-01;
									wake=rv;
								}
						}
				}
		}
	[common]
		{
			membership = automaton, monster, immune_freeze;
			[template_triggers]
				{
					[*]
						{
							action* = call_sfx_script("automaton_smoke");
							condition* = receive_world_message("WE_ENEMY_SPOTTED");
							single_shot = true;
						}
					[*]
						{
							action* = call_sfx_script("automaton_death_sparks");
							condition* = receive_world_message("WE_KILLED");
							single_shot = true;
						}
				}
		}
	[generator_in_object]
		{
			child_template_name = dsx_automaton_skeleton;
			spawn_event = WE_EXPLODED;
			spawn_chance = 1.0;
		}
		
	[defend] { defense = 162; }

	[mind]
	{
		jat_fidget				= world\ai\jobs\common\job_fidget.skrit
		?curious				= 0.75
		&wander					= 0.8
		&social					= 0.2
		&interest				= interest
		&delay_min				= 1
		&delay_max				= 6
		&still					= false;
		on_enemy_spotted_alert_friends 		= true;
		jat_brain				= world\ai\jobs\common\brain_hero.skrit
		?actor_joins_existing_party		= true
		&actor_creates_own_party		= true
		&party_template				= "party_dsx_automaton";
		com_channels				= dyn_party, automaton;
		com_range				= 14.0;
		melee_engage_range			= 14.0;
		ranged_engage_range	 		= 14.0;
		personal_space_range 			= 0.7;
		sight_range				= 14.0;
		actor_life_ratio_low_threshold 		= 0.5;
		jat_die					= world\ai\jobs\actors\evil\job_die_explode.skrit;
		jat_startup				= world\ai\jobs\common\job_startup_reveal.skrit
		?try_face				= false;
	}
	[physics]
		{
			explode_when_killed = true;
			explosion_magnitude = 4.25;
			gib_gore_good = true;
			break_effect=generic_explosion;

			[break_particulate]
				{
					frag_glb_automaton_r-01 = 2;
					frag_glb_automaton_r-02 = 2;
					frag_glb_automaton_r-03 = 2;
					frag_glb_automaton_r-04 = 1;
					frag_glb_automaton_r-05 = 1;
				}
		}
  [inventory]
  {
      drop_at_use_point = true;
      
  }
}

