[t:template,n:2W_dsx_knight_death_base]
{
	category_name = "2W_evil_d";
	doc = "2W_dsx_knight_death_base";
	specializes = base_skeleton;
	[actor]
	{
		[skills]
		{
			strength		= 32.1, 0;
			intelligence 	= 10.7, 0;
			dexterity		= 16.6, 0;
			melee			= 52, 0;
		}
	}
	[aspect]
	{
		experience_value 	= 46434;
		life				= 1121.2;
		max_life			= 1121.2;
		mana				= 190.4;
		max_mana			= 190.4;

		model = m_c_ecm_sk_pos_2;
		//		model = m_c_edm_iw_pos_1;
		//		textures:0 = b_c_edm_ph_melee;
		material = steel;
		[voice]
		{
			[die]			{ priority = high; * = s_e_die_wraith; }
			[enemy_spotted]	{ priority = high; * = s_e_call_skeleton_SED; }
			[hit_critical]	{ priority = low; * = s_e_hit_skeleton_SED; }
			[hit_glance]	{ priority = low; * = s_e_hit_skeleton_SED; }
			[hit_solid]		{ priority = low; * = s_e_hit_skeleton_SED; }
		}
	}
	[attack]
	{
		attack_range = 0.5;
		damage_max = 554.9; 
		damage_min = 381.3; 
	}
	[defend] { defense = 520.4; }

	[body]
	{
		avg_move_velocity = 2.5;
		[bone_translator]
		{
			body_anterior = bip01_head;
			body_mid = bip01_spine2;
			body_posterior = bip01_pelvis;
			kill_bone = bip01_spine2;
			shield_bone = shield_grip;
			weapon_bone = weapon_grip;
		}
		[weapon_scales]
		{
			as_single_melee = 1.5, 1.5, 1.5;
		}
		[chore_dictionary]
		{
			chore_prefix = a_c_edm_iw_fs;
			[chore_attack]
			{
				chore_stances = 0,1;
				skrit = select_attack;
				[anim_files]
					{
						00 = at;
						01 = at-02;
					}
			}
			[chore_default]
			{
				chore_stances = 0,1;
				skrit = infinite_loop;
				[anim_files] { 00=ds; }
			}
			[chore_die]
			{
				chore_stances = 0,1;
				skrit = select_transition;
				[anim_files] { 00=di; }
			}
			[chore_fidget]
			{
				chore_stances = 0,1;
				skrit = select_fidget;
				[anim_files] { 00=dff; }
			}
			[chore_walk]
			{
				chore_stances = 0,1;
				skrit = select_walk;
				[anim_files] { 00=rl; }
			}
		}
	}
	[common]
	{
		screen_name = "Death Knight"; //--
		membership = monster,skeleton; //--
	}


	[inventory]
	{
		// TAT 8/1/2003 - delete your equipped stuff
		spew_equipped_kill_count = ;
		[pcontent]
		{
			[oneof*] { es_shield_hand = sh_m_g_c_r_s_avg; } // adds 7 to armor
			[oneof*] { es_weapon_hand = ax_d_c_1h1b_avg_dsx; }
			[oneof*] { es_chest = bd_pl_f_g_c_death_knight; } // adds 1 to armor
			
			/* Generated: Tue Jun 17 11:13:15 2003
			Level 13, Strong */
			[oneof*]
			{
				[gold*]
				{
					chance = 0.2;
					min = 23335;
					max = 46517;
				}
				[oneof*]
				{
				
					chance = 0.2;
					il_main = potion_mana_large;
					il_main = potion_health_large;
				}
				[oneof*]
				{
					chance = 0.2;
					il_main = #weapon/114-158;
					il_main = #armor/55-343;
					il_main = #*/114-158;
				}
				[oneof*]
				{
					chance = 0.01;
					il_main = #weapon/-rare(1)/158-191;
					il_main = #armor/-rare(1)/262-542;
					il_main = #*/-rare(1)/158-191;
				}
				[oneof*]
				{
					chance = 0.005;
					il_main = #weapon/-unique(2)/158-191;
					il_main = #armor/-unique(2)/262-542;
					il_main = #*/-unique(2)/158-191;
				}			
			}
		}	
	}
	[mind]
	{
		jat_brain					= world\ai\jobs\common\brain_hero.skrit
			?actor_joins_existing_party	= true;
		jat_fidget					= world\ai\jobs\common\job_fidget.skrit;
	}
	[body]
	{
		armor_version = ecm_sk;
		avg_move_velocity = 1.000000;
		[bone_translator]
		{
			body_anterior = bip01_head;
			body_mid = bip01_spine2;
			body_posterior = bip01_pelvis;
			kill_bone = bip01_spine;
			shield_bone = shield_grip;
			weapon_bone = weapon_grip;
		}
		[chore_dictionary]
		{
			chore_prefix = a_c_ecm_sk_fs;
			[chore_attack]
			{
				chore_stances = 0,1,2,3,4,5,6;
				skrit = select_attack;
				[anim_files]
				{
					00 = at;
					02 = at-03;
					04 = at-05;
					05 = at-06;
				}
			}
			[chore_default]
			{
				chore_stances = 0,1,2,3,4,5,6;
				skrit = infinite_loop;
				[anim_files] { 00=dfs; }
			}
			[chore_die]
			{
				chore_stances = 0,1,2,3,4,5,6;
				skrit = select_transition;
				[anim_files] { 00=di; }
			}
			[chore_misc]
			{
				chore_stances = ignore;
				skrit = select_transition;
				[anim_files]
				{
					slp0 = a_c_ecm_sk_fs0_ds-02;
					slp1 = a_c_ecm_sk_fs1_ds-02;
					slp2 = a_c_ecm_sk_fs2_ds-02;
					slp3 = a_c_ecm_sk_fs3_ds-02;
					slp4 = a_c_ecm_sk_fs4_ds-02;
					slp5 = a_c_ecm_sk_fs5_ds-02;
					slp6 = a_c_ecm_sk_fs6_ds-02;
					wak0 = a_c_ecm_sk_fs0_rv;
					wak1 = a_c_ecm_sk_fs1_rv;
					wak2 = a_c_ecm_sk_fs2_rv;
					wak3 = a_c_ecm_sk_fs3_rv;
					wak4 = a_c_ecm_sk_fs4_rv;
					wak5 = a_c_ecm_sk_fs5_rv;
					wak6 = a_c_ecm_sk_fs6_rv;
				}
			}
			[chore_fidget]
			{
				chore_stances = 0,1,2,3,4,5,6;
				skrit = select_fidget;
				[anim_files]
				{
					00 = dff;
					01 = dff-02;
					02 = dff-03;
				}
			}
			[chore_walk]
			{
				chore_stances = 0,1,2,3,4,5,6;
				skrit = select_walk;
				[anim_files] { 00=wl; }
			}
		}
	}
}


