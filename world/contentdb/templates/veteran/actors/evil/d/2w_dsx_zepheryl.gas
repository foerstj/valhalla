[t:template,n:2W_base_ranged_zepheryl]
{
	category_name = "2W_evil_d";
	doc = "2W_dsx_base_ranged_zepheryl";
	specializes = actor_evil;
	[actor]
	{
		[skills]
			{
				strength 		= 27, 0;
				intelligence 	=  4, 0;
				dexterity 		=  14.1, 0;
				combat_magic 	= 55, 0;
				nature_magic 	= 55, 0;
			}
	}
	[aspect]
	{
		experience_value = 30363;
		force_no_render = true;
		// MRC 7/18/2003 Allow clicking on invisible objects.
		force_no_render_click = true;
		life = 193.3;
		max_life = 193.3;
		mana = 26.7;
		max_mana = 26.7;
		draw_shadow = false;
		model = m_c_edm_ga_pos_1;
		scale_base = 1;	 

		[voice]
		{
			[die]			{ priority = high;	* = s_e_die_zepheryl_SED;		}
			[enemy_spotted]	{ priority = high; * = s_e_call_zepheryl_SED; }
			[hit_critical]	{ priority = low;	* = s_e_hit_zepheryl_SED;		}
			[hit_glance]	{ priority = low;	* = s_e_hit_zepheryl_SED;		}
			[hit_solid]		{ priority = low;	* = s_e_hit_zepheryl_SED;		}
		}
	}
	[attack]
	{
		attack_range = .5;
		damage_max = 132;
		damage_min = 99;
	}

	[defend] { defense=42.2; }

	[body]
	{
		avg_move_velocity = 7.990000;
		[bone_translator]
		{
			body_anterior = bone01;
			body_mid = bone01;
			body_posterior = bone01;
			kill_bone = bone01;
			shield_bone = bone01;
			weapon_bone = bone01;
		}
		[chore_dictionary]
		{
			chore_prefix = a_c_edm_ga_fs;
			[chore_attack]
			{
				chore_stances = 0;
				skrit = select_attack;
				[anim_files]
				{
					00 = at;
					01 = at-02;
				}
			}
			[chore_magic]
			{
				chore_stances = 0;
				skrit = select_attack;
				[anim_files]
				{
					00 = mg;
				}
			}
			[chore_default]
			{
				chore_stances = 0;
				skrit = infinite_loop;
				[anim_files] { 00=ds; }
			}
			[chore_die]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files] { 00=dfs; }
			}
			[chore_fidget]
			{
				chore_stances = 0;
				skrit = select_fidget;
				[anim_files] { 00=dff; }
			}
			[chore_misc]
			{
				chore_stances = 0;
				skrit = select_transition;
				[anim_files]
				{
					slep = ds;
					wake = rv;
				}
			}
			[chore_walk]
			{
				chore_stances = 0;
				skrit = basic_walk;
				[anim_files] { 00=fl; }
			}
		}
	}
	[common]
	{
		screen_name="Zepheryl";
		membership = gargoyle, monster, immune_polymorph;
		[template_triggers]
		{
			[*]
			{
				action* = call_sfx_script("zepheryl_fire");
				condition* = receive_world_message("WE_ENTERED_WORLD");
				single_shot = true;
			}
		}
	}
	[mind]
	{
		jat_fidget = world\ai\jobs\common\job_fidget.skrit;

		melee_engage_range		= 14;
		personal_space_range	= 0.5;
		ranged_engage_range		= 14.0;
		sight_range				= 14.0;
	}
	[physics]
	{
		break_effect = zepheryl_die;
		explode_when_killed = true;
		explosion_magnitude = 2;
		fire_effect = human_physics_fire;
		fire_resistance = 0.0;
		fire_burn_threshold = 20;
		gib_gore_good = true;
			
	}

	[inventory]
	{
		drop_at_use_point = true;
		
	}	
}
